{
    "presets": [
      "@babel/preset-env",
      ["@babel/preset-react", {"runtime": "automatic"}]
    ],
    "plugins": [
        [
          "import",
          {
            "libraryName": "antd",
            "libraryDirectory": "es",
            "style": "css"
          }
        ],
        [
            "@babel/plugin-proposal-class-properties"
        ]
      ]
}